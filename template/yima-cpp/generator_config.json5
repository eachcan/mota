{
  "version": "1.0",
  "encoding": "UTF-8 with BOM",
  "templates": {
    "header": "header.template",
    "block": "block.template",
    "struct": "struct.template",
    "enum": "enum.template",
    "field_types": "field_types.template"
  },
  "type_mapping": {
    "int8": "int8_t",
    "int16": "int16_t",
    "int32": "int32_t",
    "int64": "int64_t",
    "uint8": "uint8_t",
    "uint16": "uint16_t",
    "uint32": "uint32_t",
    "uint64": "uint64_t",
    "float32": "float",
    "float64": "double",
    "string": "QString",
    "bool": "bool",
    "bytes": "QByteArray"
  },
  "accessor_format": {
    "getter_prefix": "get",
    "setter_prefix": "set",
    "pascal_case": true
  },
  "file_path": [
    {
      "path": "{{NAMESPACE}}/{{FILE_NAME}}.h",  // 完整的文件路径为 {{OUTPUT_DIR}}/{{NAMESPACE}}/{{FILE_NAME}}.h
                                                // NAMESPACE 中的 . 会被替换为 /
      "type": "file", // 以整个 .mota 文件为单位， 生成一个文件。支持的类型有： file, enum, struct, block, annotation
    }
  ]
}
